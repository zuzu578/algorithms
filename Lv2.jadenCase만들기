function solution(s) {
   const answer = [];
   let words = s.split(" ");
    
    for(let i = 0 ; i < words.length ; i++){
     if(words[i].charAt(0) !== words[i].toUpperCase()){
        answer.push(words[i].charAt(0).toUpperCase()+words[i].slice(1).toLowerCase());
     }
    }
    return answer.join(" ") ;
    
}
